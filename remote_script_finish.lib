# Simple check the backup dir still exists
if [ ! -d "$BACKUPDIR" ]; then
	echo "Backup directory $BACKUPDIR not found"
	exit 99
fi

cd "$BACKUPDIR" || exit 1

# Remove lock file for next backup
rm "$BACKUPSTATEFILE" || exit 1

# Show statistics
du -sh --time backup_[0-9]*

# All steps done!
exit 0
